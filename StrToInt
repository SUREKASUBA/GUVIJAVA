import java.util.*;
 
class strint
{
   public static void main(String args[])
   {
      int n;
	  String s;
      Scanner in = new Scanner(System.in);
 
      System.out.println("Enter a String");
      s=in.nextLine();
	  System.out.println("STRING  IS:"+s);
      n=Integer.parseInt(s);
 
      System.out.println("INTEGER IS:"+n);
   }
}
